<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">

<node name="/ch/bbv/streetlight">
  <interface name="ch.bbv.brightness">
    <property name="scaled" type="d" access="read"/>
  </interface>
  <interface name="ch.bbv.moisture">
    <property name="scaled" type="d" access="read"/>
  </interface>
  <interface name="ch.bbv.luminosity">
    <property name="scaled" type="d" access="readwrite"/>
  </interface>
  <interface name="ch.bbv.warning">
    <property name="phrase" type="s" access="readwrite"/>
  </interface>
</node>
